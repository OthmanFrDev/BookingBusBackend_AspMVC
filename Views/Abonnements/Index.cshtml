@model IEnumerable<BookingBus.Models.Abonnement>

@{
    ViewBag.Title = "Index";
}







@Html.ActionLink("create new abonnement", "create", new { id = ViewBag.id }, "")


<div class="container">
    <div class="row">

        @foreach (var item in Model)
        {<div class="col-lg-4 col-md-4 col-sm-4">
                <div class="card">
                    <div class="image">
                        <img src="http://loremflickr.com/320/150?random=4" />
                    </div>
                    <div class="card-inner">
                        <div class="header">
                            
                            <h3>@item.Navette.lieu_depart - @item.Navette.lieu_arriver</h3>
                        </div>
                        <div class="content">
                            <p>prix: @item.prix</p>
                            @Html.ActionLink("Edit", "Edit", new { id = item.id_abonnement }) |
                            @Html.ActionLink("Details", "Details", new { id = item.id_abonnement }) |
                            @Html.ActionLink("Delete", "Delete", new { id = item.id_abonnement })
                        </div>
                    </div>
                </div>
            </div>



        }
    </div>
    </div>