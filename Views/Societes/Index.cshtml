@model IEnumerable<BookingBus.Models.Abonnement>

@{
    ViewBag.Title = "Index";
}
@Html.ActionLink("create new abonnement", "creatabonnement",new { id = ViewBag.id }, "")

@Html.ActionLink("create new bus", "createbus", new { id = ViewBag.id }, "")
<div class="container">
    <div class="row">

        @foreach (var item in Model)
        {<div class="col-lg-4 col-md-4 col-sm-4">
                <div class="card">
                    <div class="image">
                        <img src="http://loremflickr.com/320/150?random=4" />
                    </div>
                    <div class="card-inner">
                        <div class="header">
                            <h2>Title</h2>
                            <h3>Sub-Head</h3>
                        </div>
                        <div class="content">
                            <p>Content area</p>
                            @Html.ActionLink("Details", "Delete", "Abonnements", new { id = item.id_abonnement }, "")
                        </div>
                    </div>
                </div>
            </div>



        }
    </div>
</div>

