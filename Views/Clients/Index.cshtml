@model IEnumerable<BookingBus.Models.Client>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Demander", "Demander", new { id = ViewBag.id }, "")
</p>
<div class="container">
    <table class="table table-bordered table-hover">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Utilisateur.nom_complet)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Utilisateur.nom_complet)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.id_utilisateur }) |
                    @Html.ActionLink("Details", "Details", new { id = item.id_utilisateur }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.id_utilisateur })
                </td>
            </tr>
        }

    </table>
</div>