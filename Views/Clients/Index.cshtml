
<div class="container">
    <div class="row justify-content-center">

        <div class="col-lg-3 col-md-6 col-sm-6">
            <div class="card card-stats">
                <div class="card-header card-header-warning card-header-icon">
                    <div class="card-icon col-xs-6">
                        <i class="fas fa-house-user"></i>
                    </div>
                    <div class="container col-xs-6">
                        <p class="card-category ">CLIENT :</p>

                        <div class="card-title">
                            <h1 class="display-1 font-weight-normal"></h1>
                        </div>
                    </div>
                </div>
                <div class="card-footer">
                    <div class="stats">
                        <i class="fas fa-house-user"></i> @Html.ActionLink("details", "Index","Effectuers", new { id = ViewBag.id }, "")
                    </div>
                </div>
            </div>
        </div>

        <div class="col-lg-3 col-md-6 col-sm-6">
            <div class="card card-stats">
                <div class="card-header card-header-success card-header-icon">
                    <div class="card-icon col-xs-6">
                        <i class="fas fa-house-user"></i>
                    </div>
                    <div class="container col-xs-6">
                        <p class="card-category ">SOCIETE:</p>
                        <div class="card-title">
                            <h1 class="display-1 font-weight-normal"></h1>
                        </div>
                    </div>
                </div>
                <div class="card-footer">
                    <div class="stats">
                        <i class="fas fa-house-user"></i>@Html.ActionLink("details", "Index","demandes", new { id =ViewBag.id  }, "")
                    </div>
                </div>
            </div>
        </div>
  

    </div>
</div>